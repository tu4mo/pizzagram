{
  "name": "pizzagram",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "deploy": "npm run build && firebase deploy",
    "dev": "vite --open",
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:format": "prettier --check .",
    "lint:style": "stylelint \"**/*.vue\"",
    "serve": "vite preview",
    "start": "npm run dev",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@sentry/vue": "9.35.0",
    "feather-icons": "4.29.2",
    "firebase": "11.10.0",
    "vue": "3.5.17",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@types/node": "22.16.2",
    "@vitejs/plugin-vue": "6.0.0",
    "@vue/runtime-dom": "3.5.17",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-vue": "10.3.0",
    "firebase-tools": "14.10.1",
    "globals": "16.3.0",
    "postcss-html": "1.8.0",
    "prettier": "3.6.2",
    "stylelint": "16.21.1",
    "stylelint-config-alphabetical-order": "1.0.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.36.0",
    "vite": "7.0.3",
    "vue-tsc": "3.0.1"
  },
  "workspaces": [
    "e2e",
    "functions"
  ]
}
