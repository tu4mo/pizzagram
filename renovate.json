{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "groupName": "patches",
      "matchUpdateTypes": ["patch"],
      "matchCurrentVersion": "!/^0/"
    },
    {
      "matchPackagePatterns": ["^@tensorflow"],
      "groupName": "tensorflow"
    }
  ],
  "schedule": ["every weekend"]
}
