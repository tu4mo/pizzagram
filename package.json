{
  "name": "pizzagram",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "deploy": "npm run build && firebase deploy",
    "dev": "vite --open",
    "format": "prettier --write .",
    "lint": "eslint --cache --ext .js,.ts,.vue .",
    "lint:format": "prettier --check .",
