{
  "name": "pizzagram",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "deploy": "npm run build && firebase deploy",
    "dev": "vite --open",
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:format": "prettier --check .",
    "lint:style": "stylelint \"**/*.vue\"",
    "serve": "vite preview",
    "start": "npm run dev",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@sentry/vue": "8.54.0",
    "feather-icons": "4.29.2",
    "firebase": "11.2.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@types/node": "22.13.1",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/runtime-dom": "3.5.13",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-vue": "9.32.0",
    "firebase-tools": "13.29.3",
    "globals": "15.14.0",
    "postcss-html": "1.8.0",
    "prettier": "3.4.2",
    "stylelint": "16.14.1",
    "stylelint-config-recommended-vue": "1.6.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.22.0",
    "vite": "6.1.0",
    "vue-tsc": "2.2.0"
  },
  "browserslist": [
    "defaults"
  ],
  "workspaces": [
    "e2e",
    "functions"
  ]
}
