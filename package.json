{
  "name": "pizzagram",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "deploy": "npm run build && firebase deploy",
    "dev": "vite --open",
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:format": "prettier --check .",
    "lint:style": "stylelint \"**/*.vue\"",
    "serve": "vite preview",
    "start": "npm run dev",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@sentry/vue": "9.12.0",
    "feather-icons": "4.29.2",
    "firebase": "11.6.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/runtime-dom": "3.5.13",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-vue": "10.0.0",
    "firebase-tools": "14.1.0",
    "globals": "16.0.0",
    "postcss-html": "1.8.0",
    "prettier": "3.5.3",
    "stylelint": "16.18.0",
    "stylelint-config-alphabetical-order": "1.0.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vite": "6.2.6",
    "vue-tsc": "2.2.8"
  },
  "workspaces": [
    "e2e",
    "functions"
  ]
}
